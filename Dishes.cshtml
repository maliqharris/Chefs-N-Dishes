@model List<Dish>

@{
    ViewData["Title"] = "All Dishes";
}

<h1>All Dishes</h1>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Chef</th>
            <th>Tastiness</th>
            <th>Calories</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var dish in Model)
        {
            <tr>
                <td>@dish.Name</td>
                <td>@(dish.Creator.FirstName + " " + dish.Creator.LastName)</td>
                <td>@dish.Tastiness</td>
                <td>@dish.Calories</td>
            </tr>
        }
    </tbody>
</table>


